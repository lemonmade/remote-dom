{
  "name": "@lemonmade/remote-dom-workspace",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm -r run build",
    "deploy": "pnpm -r run deploy",
    "develop": "pnpm --filter @lemonmade/remote-dom-run run develop",
    "format": "prettier --write --cache .",
    "lint": "prettier --check --cache .",
    "test": "vitest --passWithNoTests",
    "type-check": "tsc --build --pretty",
    "typescript.run": "tsx --conditions quilt:source",
    "typescript.watch": "tsx watch --conditions quilt:source"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.7",
    "@changesets/cli": "^2.25.0",
    "@quilted/rollup": "^0.2.20",
    "@quilted/typescript": "^0.3.0",
    "@quilted/vite": "^0.1.16",
    "@types/node": "^20.9.0",
    "prettier": "^3.0.0",
    "rollup": "^4.9.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "packageManager": "pnpm@8.10.2+sha256.487609e857f1c11780cc98dd0bfe4c8a8b11c7f23bc3a4493ac7d263d6fb6c8c"
}
